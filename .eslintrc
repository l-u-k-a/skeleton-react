extends:
  eslint-config-airbnb

parser:
  babel-eslint

settings:
  ecmascript: 6

ecmaFeatures:
  jsx: true
  modules: true
  destructuring: true
  classes: true
  forOf: true
  blockBindings: true
  arrowFunctions: true

env:
  browser: true

rules:
  indent: 0
  func-style: 0
  func-names: 0
  comma-dangle: 0
  no-console: 0
  linebreak-style: 0
  import/no-extraneous-dependencies: 0
  import/no-unresolved: 0
  import/extensions: 0
  react/jsx-filename-extension: 0
  no-eval: 2
  no-proto: 1
  no-return-assign: 1
  no-with: 1
  eqeqeq: 1
  curly: 1
  no-caller: 1
  camelcase: 1
  no-var: 1
  no-param-reassign: 1
  new-cap: 1
  no-dupe-keys: 1
  no-tabs: 0
  keyword-spacing: 0
  space-before-function-paren: 0